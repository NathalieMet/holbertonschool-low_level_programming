#include <stdlib.h>
#include <stdio.h>
#include "main.h"
/**
 * simple_print_buffer - fills the first n bytes of the memory area pointed to
 * by s with the constant byte b
 * @i : variable
 * @s : varible
 * @b : variable
 * @n : variable
 * Return: char
 */

char *_memset(char *s, char b, unsigned int n)
{
	unsigned int i;
	for (i = 0; i < n; i++)
		s[i] = b;
	return (s);
}
