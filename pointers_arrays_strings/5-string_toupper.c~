#include <stdlib.h>
#include <time.h>
#include <stdio.h>
#include "main.h"
#include "2-strlen.c"
/**
 * string_toupper - changes all lowercase letters of a string to uppercase.
 * Return: char
 */

char *string_toupper(char *)
{
        int i;

        for (i = 0; s[i] != '\0'; i++)
        {
                if (s[i] >= 'a' && s[i] <= 'z')
			s[i] = s[i] - 32;
	}
		return s;
}
