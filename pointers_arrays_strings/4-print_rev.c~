#include <stdlib.h>
#include <time.h>
#include <stdio.h>
#include "main.h"
/**
 * print_rev - prints a string, in reverse, followed by a new line.
 * @s : variable
 * Return: void
 */

void print_rev(char *s)

{
	char *end = s;

	while (*end != '\0')
	{
		end++;
	}
	end --;
	while (end >= s)
	{
		_putchar(*end);
		end--;
	}
	putchar('\n');
}
